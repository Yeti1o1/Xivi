    "dev:dev": "echo This script doesn\\'t work yet. Please use the build and start scripts instead. # bun server/index.ts",
    "build": "mkdir -p ./dist/ && vite build && cp -r ./server/* ./dist/ && cp ./vite.config.ts ./dist/vite.config.ts",
    "start": "[ -f ./.env ] && source ./.env; bun dist/index.ts",
    "dev:check": "bun tsc",
    "clean": "rm -rf dist",
    "pm2:start": "pm2 start ecosystem.config.cjs; pm2 save --force",
    "pm2:delete": "pm2 delete ecosystem.config.cjs; pm2 save --force",
    "pm2:reset": "pm2 delete ecosystem.config.cjs; pm2 save --force; pm2 start ecosystem.config.cjs; pm2 save --force",
    "pm2:restart": "pm2 restart ecosystem.config.cjs; pm2 save --force",
    "pm2:stop": "pm2 stop ecosystem.config.cjs; pm2 save --force",
    "pm2:flush": "pm2 flush; pm2 save --force",
    "cmd:corepack": "corepack enable",
    "cmd:production": "./production.sh",
    "dev:lint": "eslint .",
    "db:push": "drizzle-kit push"
  },
